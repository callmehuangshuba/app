.container {
  align-items: center;
  box-sizing: border-box;
  display: -webkit-flex;
  display: flex;
  flex-direction: column;
}
button {
  background: initial;
}
button:focus {
  outline: 0;
}
button::after {
  border: none;
}
page {
  background: #f6f6f6;
  display: -webkit-flex;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
}
